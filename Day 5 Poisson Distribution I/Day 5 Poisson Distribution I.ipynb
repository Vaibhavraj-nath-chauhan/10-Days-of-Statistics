    "lam =float(input())\n",
    "k = int(input())\n",
    "data = 0\n",
    "fact =1\n",
    "for i in range(1,k+1):\n",
    "        fact = fact*i\n",
    "data = ((lam**k)*(e**(-lam)))/fact\n",
    "print(round(data,3))"
   ]

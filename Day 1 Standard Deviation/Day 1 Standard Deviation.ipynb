   "metadata": {},
   "outputs": [],
   "source": [
    "T = int(input())\n",
    "X = list(map(int,input().split()))\n",
    "mean = sum(X)/T\n",
    "data  = 0\n",
    "for i in X:\n",
    "    data += (i-mean)**2\n",
    "    \n",
    "print(round((data/T)**(1/2),1))"
   ]
  }
